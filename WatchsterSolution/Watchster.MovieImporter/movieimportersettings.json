{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    },
    "File": {
      "BasePath": "Logs",
      "FileAccessMode": "KeepOpenAndAutoFlush",
      "FileEncodingName": "utf-8",
      "DateFormat": "yyyyMMdd",
      "CounterFormat": "000",
      "MaxFileSize": 10485760,
      "LogLevel": {
        "Watchster.MovieImporter": "Information",
        "Default": "Debug"
      },
      "IncludeScopes": true,
      "MaxQueueSize": 100,
      "Files": [
        {
          "Path": "log-<counter>.log"
        },
        {
          "Path": "<date:yyyy>/<date:MM>/log-<counter>.log",
          "MinLevel": {
            "Watchster.MovieImporter": "Warning",
            "Default": "Trace"
          },
          "CounterFormat": "00"
        }
      ]
    }
  },
  "QuartzSettings": {
    "StartAt": null,
    "RerunUnitOfMeasureIsInHours": true,
    "RerunInHours": 24,
    "RerunInMinutest": 1
  },
  "ConnectionStrings": {
    "WatchsterDB": "Server=localhost;Database=WatchsterDB;Trusted_Connection=True;"
  },
  "Authentication": {
    "Issuer": "Watchster",
    "MinutesExpiration": 1440,
    "Key": ""
  }
}